{
    "cli": {
      "version": ">= 5.9.0"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "env": {
          "APP_ENV": "development"
        },
        "android": {
          "gradleCommand": ":app:assembleDebug",
          "buildType": "apk"
        },
        "ios": {
          "buildConfiguration": "Debug"
        },
        "channel": "development"
      },
      "development-local": {
        "extends": "development",
        "distribution": "internal",
        "android": {
          "buildType": "apk"
        },
        "ios": {
          "simulator": true,
          "buildConfiguration": "Debug"
        }
      },
      "preview": {
        "distribution": "internal",
        "env": {
          "APP_ENV": "development"
        },
        "android": {
          "buildType": "apk"
        },
        "channel": "preview"
      },
      "production": {
        "env": {
          "APP_ENV": "production"
        },
        "android": {
          "buildType": "apk"
        },
        "channel": "production"
      },
      "production-store": {
        "extends": "production",
        "android": {
          "buildType": "app-bundle"
        },
        "ios": {
          "buildConfiguration": "Release"
        }
      }
    },
    "submit": {
      "production": {}
    }
  }