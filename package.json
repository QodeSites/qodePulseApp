{
  "name": "qodepulse",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --tunnel --clear",
    "android": "expo start --android --tunnel --clear",
    "ios": "expo start --ios --tunnel --clear",
    "web": "expo start --web --tunnel --clear",
    "reset": "expo start -c --tunnel --clear",
    "build:dev:local:android": "APP_ENV=development eas build --profile development-local --platform android --local",
    "build:dev:local:ios": "APP_ENV=development eas build --profile development-local --platform ios --local",
    "build:dev:cloud:android": "APP_ENV=development eas build --profile development --platform android",
    "build:dev:cloud:ios": "APP_ENV=development eas build --profile development --platform ios",
    "build:preview:android": "APP_ENV=development eas build --profile preview --platform android",
    "build:preview:ios": "APP_ENV=development eas build --profile preview --platform ios",
    "build:prod:android": "APP_ENV=production eas build --profile production --platform android",
    "build:prod:ios": "APP_ENV=production eas build --profile production --platform ios",
    "build:prod:store:android": "APP_ENV=production eas build --profile production-store --platform android",
    "build:prod:store:ios": "APP_ENV=production eas build --profile production-store --platform ios"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "axios": "^1.13.2",
    "expo": "~54.0.31",
    "expo-auth-session": "^7.0.10",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-secure-store": "^15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.16",
    "expo-web-browser": "^15.0.10",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "@types/react": "~19.1.0",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
